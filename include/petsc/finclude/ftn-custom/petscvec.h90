      Interface VecSetValues
        subroutine VecSetValues0(a,b,c,d,e,z)
          USE_VEC_HIDE
          VEC_HIDE a ! Vec
          PetscInt b ! PetscInt
          PetscInt c (*) ! PetscInt
          PetscScalar d (*) ! PetscScalar
          InsertMode e ! InsertMode
          integer z
        end subroutine
