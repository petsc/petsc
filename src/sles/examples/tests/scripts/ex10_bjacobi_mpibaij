#! /bin/csh

foreach matrix (arco1)
  foreach blocks (1 2 3 6) 
    foreach np (1 2 3 4 )
      echo "matrix $matrix blocksize $blocks np $np"
      mpirun -np $np ../ex10 -f0 /home/bsmith/petsc/src/mat/examples/matrices/$matrix -pc_type bjacobi -mat_baij -matload_block_size $blocks  -ksp_monitor 
    end
  end
end
