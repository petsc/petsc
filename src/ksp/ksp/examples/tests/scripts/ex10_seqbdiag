#! /bin/csh
#
#  Tests SeqBdiag matrix format for different blocksizes
#
foreach matrix (medium arco1)

  foreach mode (-mat_no_unroll nothing)

    foreach bs (4 5)

      mpirun ex10 -f0 /home/bsmith/petsc/src/mat/examples/matrices/$matrix -pc_type ilu -mat_order natural -ksp_monitor $mode -matload_ignore_info -matload_block_size $bs -optionsleft -mat_seqbdiag -ksp_gmres_restart 200

    end

  end

end
