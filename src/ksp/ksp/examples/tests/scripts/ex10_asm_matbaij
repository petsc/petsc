#! /bin/csh

foreach matrix (small)

  foreach blocks (1 2 3 6) 

    foreach overlap (0 1 2) 

      echo "matrix $matrix blocks $blocks overlap $overlap"
      mpirun ../ex10 -f0 /home/bsmith/petsc/src/mat/examples/matrices/$matrix -pc_type asm -mat_seqbaij -matload_block_size 6 -pc_asm_blocks $blocks -pc_asm_overlap $overlap -ksp_monitor >! ex10_asm_matbaij_1.log
        mpirun ../ex10 -f0 /home/bsmith/petsc/src/mat/examples/matrices/$matrix -pc_type asm -mat_seqaij -pc_asm_blocks $blocks -pc_asm_overlap $overlap -ksp_monitor >! ex10_asm_matbaij_2.log
        diff ex10_asm_matbaij_1.log  ex10_asm_matbaij_1.log
        /bin/rm -f ex10_asm_matbaij_1.log  ex10_asm_matbaij_1.log
    end

  end

end
