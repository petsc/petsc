

      Interface
      subroutine KSPSetType(a,b,z)
      use petsckspdef
       KSP a
       character(*) b
       PetscErrorCode z
       end subroutine
      end Interface

      Interface
      subroutine KSPView(a,b,z)
      use petsckspdef
       KSP a
       PetscViewer b
       PetscErrorCode z
       end subroutine
      end Interface

