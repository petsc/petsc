	  ${DIFF} output/ex40_2.out ex40_2.tmp || printf "${PWD}\nPossible problem with ex40_2, diffs above\n=========================================\n"; \
	  ${RM} -f ex40_2.tmp
runex42:
	-@${MPIEXEC} -n 2 ./ex42 ;\
	  ${RM} -f 2d_nocoord.vtr 2d.vtr 3d_nocoord.vtr 3d.vtr
runex43:
