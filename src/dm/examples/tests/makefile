	-@${MPIEXEC} -n 12 ./ex34 -dm_view -dim 3 -da_grid_x 11 -da_grid_y 5 -da_grid_z 7 | grep -v "type:" | sed "s/\[//g" | sed "s/\]//g" | LC_ALL=C sort -b -d > ex34_1.tmp; \
